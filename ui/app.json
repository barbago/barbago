{
  "expo": {
    "name": "Barbago Mobile",
    "slug": "barbago-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icons/icon.png",
    "scheme": "barbago",
    "userInterfaceStyle": "automatic",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "splash": {
      "image": "./src/assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "url": "https://u.expo.dev/5ac3abb8-36b7-4e76-b88f-33c97c351d11",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you choose a profile picture and upload testimonials or service images."
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "app.barbago.mobile",
      "supportsTablet": true,
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDFfOOF2bglIb79qcPFQgd8Rzmo4mwLKF4"
        }
      },
      "package": "app.barbago.mobile"
    },
    "web": {
      "build": {
        "babel": {
          "include": ["react-leaflet"]
        }
      },
      "favicon": "./src/assets/icons/favicon.png"
    },
    "description": "",
    "githubUrl": "https://github.com/barbago/barago-mobile",
    "extra": {
      "eas": {
        "projectId": "5ac3abb8-36b7-4e76-b88f-33c97c351d11"
      }
    }
  }
}
