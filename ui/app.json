{
  "expo": {
    "name": "Barbago Mobile",
    "slug": "barbago-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "description": "",
    "githubUrl": "https://github.com/barbago/barago",
    "icon": "./src/assets/icons/icon.png",
    "scheme": "barbago",
    "userInterfaceStyle": "automatic",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "splash": {
      "image": "./src/assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "url": "https://u.expo.dev/4237973e-792d-4779-a1a4-d58739bc9823",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps"
        }
      ]
    },
    "plugins": [
      "expo-apple-authentication",
      "sentry-expo",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you choose a profile picture and upload testimonials or service images."
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "app.barbago.mobile",
      "supportsTablet": true,
      "usesAppleSignIn": true
    },
    "android": {
      "package": "app.barbago.mobile",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          // make this dynamic instead?
          // find out if you can use env for config.ts
          "apiKey": "AIzaSyDFfOOF2bglIb79qcPFQgd8Rzmo4mwLKF4"
        }
      }
    },
    "web": {
      "build": {
        "babel": {
          "include": ["react-leaflet", "@sentry/react-native"]
        }
      },
      "favicon": "./src/assets/icons/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "4237973e-792d-4779-a1a4-d58739bc9823"
      }
    }
  }
}
